<p id="notice"><%= notice %></p>

<h1>Cita</h1>

<table>
  <thead>
    <tr>
      <th>Id paciente</th>
      <th>Nombre paciente</th>
      <th>Id doctor</th>
      <th>Nombre doctor</th>
      <th>Consultorio</th>
      <th>Fecha hora</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cita.each do |cita| %>
      <tr>
        <td><%= cita.id_paciente %></td>
        <td><%= cita.nombre_paciente %></td>
        <td><%= cita.id_doctor %></td>
        <td><%= cita.nombre_doctor %></td>
        <td><%= cita.consultorio %></td>
        <td><%= cita.fecha_hora %></td>
        <td><%= link_to 'Show', cita %></td>
        <td><%= link_to 'Edit', edit_cita_path(cita) %></td>
        <td><%= link_to 'Destroy', cita, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cita', new_cita_path %>
